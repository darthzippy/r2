<table cellspacing="0">
  <tr>
    <th>First</th>
    <th>Last</th>
    <th>Role</th>
	<th>Email</th>
	<th></th>
	<th></th>
	<th></th>
  </tr>

<% for user in @users %>
  <tr>
    <td><%=h user.first %></td>
    <td><%=h user.last %></td>
    <td><%=h user.role %></td>
	<td><%=h user.email  %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

